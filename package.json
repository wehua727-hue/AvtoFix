    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@swc/core": "^1.15.2",
    "@tailwindcss/postcss": "^4.1.17",
    "@tailwindcss/typography": "^0.5.19",
    "@tanstack/react-query": "^5.90.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
