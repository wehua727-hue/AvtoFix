{
  "name": "oflayn-dokon",
  "version": "1.0.0",
  "description": "Offline auto parts store web app built with React and Express.",
  "author": "OflaynDokon",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently -k -n VITE,API -c green,cyan \"pnpm run dev:vite\" \"pnpm run dev:server\"",
    "dev:vite": "vite",
    "dev:server": "tsx watch server/node-build.ts",
    "build": "pnpm run build:client && pnpm run build:server",
    "build:client": "vite build",
    "build:server": "vite build --config vite.config.server.ts",
    "start": "node dist/server/production.mjs",
    "pm2:start": "pm2 start ecosystem.config.cjs",
    "pm2:stop": "pm2 stop oflayn-dokon",
    "pm2:restart": "pm2 restart oflayn-dokon",
    "pm2:logs": "pm2 logs oflayn-dokon",
    "pm2:delete": "pm2 delete oflayn-dokon",
    "pm2:save": "pm2 save",
    "pm2:status": "pm2 status",
    "pm2:monit": "pm2 monit",
    "test": "vitest --run",
    "format.fix": "prettier --write .",
    "typecheck": "tsc"
  },
