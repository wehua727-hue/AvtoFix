{
  "name": "oflayn-dokon",
  "version": "1.0.0",
  "description": "Offline auto parts store web app built with React and Express.",
  "author": "OflaynDokon",
  "private": true,
  "type": "module",
  "main": "electron/main.cjs",
  "scripts": {
    "dev": "concurrently -k -n VITE,API -c green,cyan \"pnpm run dev:vite\" \"pnpm run dev:server\"",
    "dev:vite": "vite",
    "dev:server": "tsx watch server/node-build.ts",
    "tunnel": "lt --port 5173 --subdomain avtofix-backend",
    "dev:with-tunnel": "concurrently -k -n VITE,API,TUNNEL -c green,cyan,magenta \"pnpm run dev:vite\" \"pnpm run dev:server\" \"pnpm run tunnel\"",
    "build": "pnpm run build:client && pnpm run build:server",
    "build:client": "vite build",
    "build:server": "vite build --config vite.config.server.ts",
    "build:production": "pnpm run build:client",
    "prebuild:production": "echo Building for production... && echo Remember to upload dist/ folder to WPS hosting!",
    "deploy:wps": "bash wps-deploy.sh",
    "deploy:wps:win": "wps-deploy.bat",
    "start": "node dist/server/production.mjs",
    "pm2:start": "pm2 start ecosystem.config.cjs",
    "pm2:stop": "pm2 stop oflayn-dokon",
    "pm2:restart": "pm2 restart oflayn-dokon",
    "pm2:logs": "pm2 logs oflayn-dokon",
    "pm2:delete": "pm2 delete oflayn-dokon",
    "pm2:save": "pm2 save",
    "pm2:status": "pm2 status",
    "pm2:monit": "pm2 monit",
    "test": "vitest --run",
    "format.fix": "prettier --write .",
    "typecheck": "tsc",
    "create-admin": "tsx server/scripts/create-admin-user.ts",
    "create-owner": "tsx server/scripts/create-owner.ts",
    "restore-users": "tsx server/scripts/restore-users.ts",
    "create-test-orders": "tsx server/scripts/create-test-orders.ts",
    "check-userid": "tsx server/scripts/check-debts-userid.ts",
    "migrate-userid": "tsx server/scripts/migrate-debts-userid.ts",
    "migrate-categories": "tsx server/scripts/migrate-categories-add-userId.ts",
    "test-categories": "tsx server/scripts/test-categories-separation.ts",
    "test-subscription-notifications": "tsx server/scripts/test-subscription-notifications.ts",
    "unblock-user": "tsx server/scripts/unblock-user.ts",
    "check-products-by-user": "tsx server/scripts/check-products-by-user.ts",
    "assign-products-to-user": "tsx server/scripts/assign-products-to-user.ts",
    "unassign-products": "tsx server/scripts/unassign-products.ts",
    "fix-excel-initial-stock": "tsx server/scripts/fix-excel-initial-stock.ts",
    "fix-code-catalog": "tsx server/scripts/fix-code-catalogNumber.ts",
    "fix-excel-code-catalog": "tsx server/scripts/fix-excel-code-catalog.ts",
    "find-duplicate-skus": "tsx server/scripts/find-duplicate-skus.ts",
    "electron:dev": "concurrently -k -n VITE,API,ELECTRON -c green,cyan,blue \"pnpm run dev:vite\" \"pnpm run dev:server\" \"wait-on http://localhost:5174 && cross-env NODE_ENV=development electron .\"",
    "electron:start": "electron .",
    "electron:build:win": "pnpm run build:client && electron-builder --win --config electron-builder.config.cjs",
    "electron:build:mac": "pnpm run build:client && electron-builder --mac --config electron-builder.config.cjs",
    "electron:build:linux": "pnpm run build:client && electron-builder --linux --config electron-builder.config.cjs"
  },
  "dependencies": {
    "@radix-ui/react-tooltip": "^1.2.8",
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.4.2",
